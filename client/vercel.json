{
  "version": 2,
  "builds": [
    {
      "src": "client/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "client/src/**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "client/src/style.css",
      "use": "@vercel/static"
    },
    {
      "src": "client/assets/images/logo.png",
      "use": "@vercel/static"
    },
    {
      "src": "client/public/favicon.ico",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/favicon.ico", "dest": "/client/public/favicon.ico" },
    { "src": "/src/style.css", "dest": "/client/src/style.css" },
    { "src": "/src/index.js", "dest": "/client/src/index.js" },
    { "src": "/src/components/(.*)", "dest": "/client/src/components/$1" },
    { "src": "/src/utils/(.*)", "dest": "/client/src/utils/$1" },
    { "src": "/assets/images/logo.png", "dest": "/client/assets/images/logo.png" },
    { "src": "/(.*)", "dest": "/client/index.html" }
  ]
}
